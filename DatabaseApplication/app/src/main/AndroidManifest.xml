        </activity>
        <activity android:name=".InsertActivity">
        </activity>
        <activity android:name=".UpdateActivity">
        </activity>
        <activity android:name=".DeleteActivity">
        </activity>
        <activity android:name=".RetrieveActivity">
        </activity>
        <activity android:name=".RetrieveDeptActivity">
        </activity>
    </application>

</manifest>